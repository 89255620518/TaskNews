{
  "name": "server",
  "version": "1.0.0",
  "description": "Node.js Server with TypeScript",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:tsnode": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "clean": "rm -rf dist",
    "migrate": "sequelize-cli db:migrate",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "sequelize"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "connect": "^3.7.0",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "tsx": "^4.19.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/body-parser": "^1.19.5",
    "@types/connect": "^3.4.38",
    "@types/continuation-local-storage": "^3.2.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/send": "^0.17.4",
    "@types/serve-static": "^1.15.7",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
